#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"

# Then outputs the board and prompts for input
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"
input = gets.strip
possition_on_the_board = input_to_index(input)

move(board, possition_on_the_board)

display_board(board)

